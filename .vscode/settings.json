{
  /* Tailwindcss Setting */
  // "css.validate": false,
  // "editor.quickSuggestions": {
  //   "strings": true
  // },
  // "tailwindCSS.includeLanguages": {
  //   "plaintext": "html"
  // },
  // "http.proxySupport": "off",
  "prettier.enable": false,
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.organizeImports": false
  },

  // The following is optional.
  // It's better to put under project setting `.vscode/settings.json`
  // to avoid conflicts with working with different eslint configs
  // that does not support all formats.
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue",
    "html",
    "markdown",
    "json",
    "jsonc",
    // "yaml"
  ],
  "editor.hover.enabled": true,
  "files.associations": {
    "*.css": "postcss"
  },
  // "volar.completion.preferredTagNameCase": "pascal",
  // "volar.completion.preferredAttrNameCase": "kebab",
  // "editor.defaultFormatter": "esbenp.prettier-vscode",
  // "githubIssues.issueBranchTitle": "TPL#${issueNumber}",
  "nuxtr.vueFiles.firstTag": "script",
  "nuxtr.vueFiles.script.type": "setup",
  "nuxtr.vueFiles.script.defaultLanguage": "ts",
  "nuxtr.vueFiles.style.addStyleTag": false,
  "nuxtr.vueFiles.style.alwaysScoped": true,
  "nuxtr.vueFiles.style.defaultLanguage": "css",
  "githubPullRequests.assignCreated": "${user}",
  "githubPullRequests.postCreate": "checkoutDefaultBranch",
  "githubIssues.issueBranchTitle": "${user}/issue#${issueNumber}"
}
